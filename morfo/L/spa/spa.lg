name: castellano

seg: a b d e f g h i j
     m n ñ o p s t u v
     x y z á é í ó ú ü
     k w
     c ch l ll qu r rr
     A B D E F G H I J
     M N Ñ O P S T U V
     X Y Z Á É Í Ó Ú Ü
     K W
     C Ch L Ll Qu R
     ! ' ` _

accent:   a:á, e:é, i:í, o:ó, u:ú
deaccent: á:a, é:e, í:i, ó:o, ú:u

feats:

## SUSTANTIVOS
  pos: n sust

  n (num) = s | p
  g (gen) = m | f
  xf: g = gen
  xf: n = num
  ab: p = plur
  ab: s = sing
  ab: m = mas
  ab: f = fem

## ADJETIVOS
  pos: a adj

  n (num) = s | p
  g (gen) = m | f
  xf: g = gen
  xf: n = num
  ab: m = mas
  ab: f = fem
  ab: p = plur
  ab: s = sing

## VERBOS
  pos: v vrb

  # Si un complemento no es explícito, no se muestran otras propiedades
  !-xpl = ninguno
  ab: prs = pres
  ab: prt = pret
  ab: cnd = cond
  ab: fut = fut
  ab: ipf = imprf
  ab: ipv = imper
  ab: sbp = subj pres
  ab: sbi = subj imprf
  ab: inf = inf
  ab: ger = ger
  ab: prc = part

  xf: p = persona
  xf: tm = tmp/asp/mod
  xf: n = número
  xf: po = persona de objeto
  xf: no = número de objeto
#  xf~: VOS = voseo

  p (persona) = 3 | 2 | 1

  n (número) = 0 | 1

  tm (tmp/asp/mod) = prs | fut | prt | sbp | sbi | ipf | cnd | ipv | ger | inf | prc

  %*fin (finito) = *True | False
  %*ipv (imper) = *False | True

  VOS = *True | False
  AL = *True | *False

