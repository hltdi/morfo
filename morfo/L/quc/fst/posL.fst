# L form of positional verbs
# first derivational rule
# CVC -> CVCVl/n
# CV' -> CV'l/n

-> start

start -> start [C;V;P;S-L; ]

start -> C     [C-',l,r]
# insert reduplicated vowel       
C -> CV        [v:]
CV -> start    [l:L]

start -> r     [l;r]
# insert reduplicated vowel
r -> rV        [v:]          
rV -> start    [n:L]

start -> '     [']
# no second vowel following '
' -> start     [l:L]

start ->
