$ sbj_pre
  e   [sp=1,sn=1,-neg,fut=def];[sp=1,sn=1,fut=indef]
  t   [sp=3,sn=1,sg=f,misc=[-tconv]];[sp=2,fut=indef]
  y   [sp=3,sn=2,+neg];[sp=3,sn=1,sg=m,+neg];[sp=None,sn=None,sg=None,+neg];
      [sp=3,sn=2,fut=def|indef];[sp=3,sn=1,sg=m,fut=def|indef];[sp=None,sn=None,sg=None,fut=def|indef]
  n   [sp=1,sn=2,misc=[-tconv]];[sp=1,sn=1,misc=[-tconv]]
  ey  [sp=3,sn=2,-neg,fut=None,misc=[-tconv]];[sp=3,sn=1,sg=m,-neg,fut=None,misc=[-tconv]];[sp=None,sn=None,sg=None,-neg,fut=None,misc=[-tconv]]
  --  [sp=2,fut=None,-neg];[misc=[+tconv]]

